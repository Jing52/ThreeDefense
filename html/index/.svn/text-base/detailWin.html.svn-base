	<script type="text/javascript">
		$(function(){
			$('.close').click(function(){
				parent.hideWin();
			});
			loadChart();
		});
		function loadChart(){
			//来源总览
			var myFlow = echarts.init(document.getElementById('chart'));
			var option = {
			    tooltip: {
			        trigger: 'item',
			        formatter: "{a} <br/>{b}: {c}% "
			    },
			    legend:{
			    	show:true,
			    	orient:'vertical',
			    	left:'80%',
			    	top:'10%',
			    	itemWidth :14,
			    	itemHeight :14,
			    	data:['国际','国内','省内','本地'],
			    	textStyle:{
			    		color:'#333',
			    		fontSize:'14',
			    	}
			    },
			    series: [
			        {
			            name:'来源',
			            type:'pie',
			            radius: ['0', '65%'],
			            center: ['40%', '50%'],
			            data:[
			            	{value:16, name:'国际'},
			                {value:35, name:'国内'},
			                {value:29, name:'省内'},
			                {value:20, name:'本地'},
			            ],
			            color:['#d25000','#ffa912','#258d8c','#0559a6'],
			            itemStyle: {
			            	normal:{
			            		label: {
							        show: true,
							        color: '#fdfdfd',
                    			    formatter: '{c}%',
                    			    textStyle :{
                    			   		fontSize:'18',
                    			   		fontWeight:'bold'
                    			   	}
							    },
			            		labelLine: {
							       show: false,
							       length:3
							    }
			            	},
			                emphasis: {
			                    shadowBlur: 10,
			                    shadowOffsetX: 0,
			                    shadowColor: 'rgba(0, 0, 0, 0.5)'
			                }
			            }
			        }
			    ]
			}
			myFlow.setOption(option);
		}
	</script>
